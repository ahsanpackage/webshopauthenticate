{
    "name": "ahsanpackage/webshopauthenticate",
    "description": "A Laravel 4 package for basic authentication",
    "authors": [
        {
            "name": "Senthil vasan",
            "email": "rsensan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "cartalyst/sentry": "2.0.*"
    },
    "autoload": {
